{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": { "^.+\\.(t|j)s$": "ts-jest" },
  "transformIgnorePatterns": ["node_modules/(?!(.pnpm|uuid)/)"],
  "setupFiles": ["./setup.ts"],
  "moduleNameMapper": { "^src/(.*)$": "<rootDir>/../src/$1" },
  "maxWorkers": 1
}
